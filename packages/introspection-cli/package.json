{
  "name": "@ptsq/introspection-cli",
  "version": "0.3.3",
  "homepage": "https://ptsq.vercel.app/",
  "author": "lewis-wow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lewis-wow/ptsq.git",
    "directory": "packages/introspection-cli"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "rollup --config rollup.config.ts --bundleConfigAsCjs",
    "dev": "ts-node src/main.ts",
    "start": "node dist/main.js",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "bin": {
    "introspection-cli": "./dist/main.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/main.js",
  "module": "dist/main.mjs",
  "typings": "dist/main.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.js",
      "default": "./dist/main.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "package.json",
    "!**/*.test.*"
  ],
  "devDependencies": {
    "@sinclair/typebox": "^0.31.28",
    "@types/node": "^20.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "@ptsq/server": "*"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "json-schema-to-typescript": "^13.1.1",
    "yargs": "^17.7.2"
  }
}
