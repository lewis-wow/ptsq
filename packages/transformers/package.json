{
  "name": "@ptsq/transformers",
  "version": "0.2.2",
  "homepage": "https://ptsq-docs.vercel.app/",
  "author": "lewis-wow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lewis-wow/ptsq.git",
    "directory": "packages/transformers"
  },
  "scripts": {
    "prebuild": "rm -rf dist && barrelsby --name main --delete --singleQuotes --directory src --exclude \".test.\"",
    "build": "rollup --config rollup.config.ts --bundleConfigAsCjs",
    "lint": "eslint",
    "test": "vitest"
  },
  "main": "dist/main.js",
  "module": "dist/main.mjs",
  "typings": "dist/main.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "package.json",
    "!**/*.test.*"
  ],
  "dependencies": {
    "@ptsq/server": "*",
    "awesome-phonenumber": "^6.2.0",
    "country-code-lookup": "^0.1.0",
    "parse-color": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.7.0",
    "@types/parse-color": "^1.0.3",
    "barrelsby": "^2.8.1",
    "typescript": "^5.2.2"
  }
}
