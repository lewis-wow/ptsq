  "devDependencies": {
    "@ptsq/test-utils": "*",
    "@types/node": "^20.7.0",
    "@types/uuid": "^9.0.7",
    "axios": "^1.5.1",
    "json-schema-to-ts": "^3.0.1",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.31.28",
    "@whatwg-node/server": "^0.9.34"
  },
