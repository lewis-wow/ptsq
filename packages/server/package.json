  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lewis-wow/ptsq.git",
    "directory": "packages/server"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "rollup --config rollup.config.ts --bundleConfigAsCjs",
