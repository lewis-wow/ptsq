{
  "name": "@ptsq/server",
  "version": "0.0.2",
  "homepage": "https://ptsq-docs.vercel.app/",
  "author": "lewis-wow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lewis-wow/ptsq.git",
    "directory": "packages/server"
  },
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint",
    "test": "vitest"
  },
  "main": "dist/main.js",
  "module": "dist/main.mjs",
  "typings": "dist/main.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.js",
      "default": "./dist/main.js"
    },
    "./adapters/express": {
      "import": "./dist/adapters/express.mjs",
      "require": "./dist/adapters/express.js",
      "default": "./dist/adapters/express.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "package.json",
    "adapters",
    "!**/*.test.*"
  ],
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/koa": "^2.13.10",
    "@types/koa-bodyparser": "^4.3.11",
    "@types/koa-router": "^7.4.6",
    "@types/koa__cors": "^4.0.2",
    "@types/node": "^20.7.0",
    "@types/pascalcase": "^1.0.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^11.0.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "pascalcase": "^0.1.1",
    "vitest": "^0.34.6",
    "zod": "^3.22.2",
    "zod-to-json-schema": "^3.21.4"
  }
}
