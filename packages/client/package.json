{
  "name": "@ptsq/client",
  "version": "0.2.3",
  "homepage": "https://ptsq-docs.vercel.app/",
  "author": "lewis-wow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lewis-wow/ptsq.git",
    "directory": "packages/client"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "rollup --config rollup.config.ts --bundleConfigAsCjs",
    "lint": "eslint",
    "test": "vitest"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "src/main.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.js",
      "default": "./dist/main.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "package.json",
    "!**/*.test.*"
  ],
  "devDependencies": {
    "@ptsq/server": "*",
    "@types/node": "^20.7.0",
    "typescript": "^5.2.2",
    "@ptsq/test-utils": "*",
    "zod": "^3.22.2"
  },
  "dependencies": {
    "axios": "^1.5.1"
  }
}
