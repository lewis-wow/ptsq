{
  "name": "@ptsq/example-sveltekit",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "generate:schema": "introspection-cli --url='http://localhost:5173/api/ptsq' --out='src/generated/schema.generated.ts'"
  },
  "dependencies": {
    "@ptsq/server": "latest",
    "@ptsq/svelte-client": "latest",
    "@sveltejs/adapter-node": "^5.0.1",
    "@tanstack/svelte-query": "^5.18.0",
    "@sinclair/typebox": "^0.31.28"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-svelte": "^2.35.1",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "@ptsq/introspection-cli": "latest"
  },
  "type": "module"
}
