{
  "name": "@ptsq/example-express-prisma",
  "version": "0.0.0",
  "private": true,
  "main": "src/server.ts",
  "scripts": {
    "prebuild": "prisma generate",
    "postinstall": "prisma generate",
    "build": "tsc --build",
    "start:server": "dotenv -e .env -- ts-node src/server.ts",
    "start:client": "ts-node src/client.ts",
    "generate:schema": "introspection-cli --url='http://localhost:4000/ptsq'",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate"
  },
  "devDependencies": {
    "@ptsq/introspection-cli": "*",
    "@types/express": "^4.17.18",
    "@types/node": "^20.7.0",
    "dotenv-cli": "^7.3.0",
    "prisma": "5.9.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@prisma/client": "5.9.1",
    "@ptsq/client": "latest",
    "@ptsq/server": "latest",
    "express": "^4.18.2"
  }
}
