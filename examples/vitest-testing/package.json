  "devDependencies": {
    "@ptsq/introspection-cli": "latest",
    "@types/node": "^20.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@ptsq/client": "latest",
    "@ptsq/server": "latest"
  }
}
