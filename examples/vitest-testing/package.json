{
  "name": "@ptsq/example-vitest-testing",
  "version": "0.0.0",
  "private": true,
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc --build",
    "start:server": "ts-node src/server.ts",
    "start:client": "ts-node src/client.ts",
