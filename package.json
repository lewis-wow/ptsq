{
  "name": "ptsq",
  "private": true,
  "packageManager": "pnpm@8.15.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tanstack/svelte-query": "^5.18.0",
    "@testing-library/jest-dom": "^6.4.1",
    "@testing-library/svelte": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitest/coverage-istanbul": "^0.34.6",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vitest": "^0.3.2",
    "jsdom": "^24.0.0",
    "prettier": "^3.0.3",
    "rollup": "^4.1.5",
    "svelte-preprocess": "^5.1.3",
    "tsconfig": "*",
    "turbo": "latest",
    "vitest": "^0.34.6"
  },
  "pnpm": {
    "overrides": {
      "@ptsq/example-next>@prisma/client": "https://registry.npmjs.com/@prisma/client/-/client-5.9.1.tgz?id=example-next",
      "@ptsq/example-express-prisma>@prisma/client": "https://registry.npmjs.com/@prisma/client/-/client-5.9.1.tgz?id=example-express-prisma"
    }
  }
}
